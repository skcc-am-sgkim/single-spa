{
  "name": "@bcp/frontend-root",
  "scripts": {
    "dev": "concurrently 'npm:start*'",
    "start": "webpack serve --port 9000 --env isLocal",
    "start-gnb": "cd ../bcp-frontend-gnb && npm start",
    "start-snb": "cd ../bcp-frontend-snb && npm start",
    "start-mdi": "cd ../bcp-frontend-mdi && npm start",
    "start-nexacro": "cd ../bcp-frontend-nexacro && npm start",
    "start-shared": "cd ../bcp-frontend-shared && npm start",
    "start-react": "cd ../bcp-frontend-standard-react && npm start",
    "start-parcel-react": "cd ../bcp-frontend-parcel-react && npm start",
    "start-parcel-nexacro": "cd ../bcp-frontend-parcel-nexacro && npm start",
    "start-parcel-vue": "cd ../bcp-frontend-parcel-vue && npm run serve",
    "lint": "eslint src --ext js",
    "test": "cross-env BABEL_ENV=test jest --passWithNoTests",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "build": "concurrently yarn:build:*",
    "build:webpack": "webpack --mode=production"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/runtime": "^7.15.3",
    "autoprefixer": "^10.4.15",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-important-stuff": "^1.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.0.6",
    "jest-cli": "^27.0.6",
    "postcss": "^8.4.29",
    "postcss-loader": "^7.3.3",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "serve": "^14.2.1",
    "tailwindcss": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-config-single-spa": "^5.0.0",
    "webpack-dev-server": "^4.0.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@types/jest": "^27.0.1",
    "@types/systemjs": "^6.1.1",
    "single-spa": "^5.9.3",
    "single-spa-layout": "^1.6.0"
  }
}
